    <style>
        .right-nav {
            width: 333px;
            background-color: #10122d;
            position: absolute;
        }

        .right-nav__title {
            color: #f2f4ff;

            font-size: 16px;
            font-weight: 300;
            text-transform: uppercase;
            padding: 32px 0 28px 48px;
        }

        .right-nav__show {
            position: absolute;
            right: 16px;
            top: 27px;
            width: 24px;
            height: 24px;
            transition: 0.3s all ease;
        }

        .pa-navigation {
            padding-bottom: 70px;
        }

        .clearfix {
            zoom: 1;
        }

        .nav-top__soc--right {
            text-align: center;
            margin: 145px 0 24px;
        }

        .nav-top__soc {
            font-size: 0;
            position: relative;
            z-index: 10;
        }

        .right-nav__copy {
            color: #f2f4ff;

            font-size: 13px;
            font-weight: 400;
            text-align: center;
            padding: 0 5px;
        }

        .right-nav__links {
            text-align: center;
            margin: 16px 0;
        }

        .page {
            position: relative;
            /* background: #f2f2f2 url(../img/new/career_back.png) center center no-repeat; */
            padding-top: 80px;
            background-size: contain;
        }

        .page--pa {
            /* background: #222222; */
            background-size: 100% auto;
            padding-top: 0;
        }

        .pa-navigation__link_i {
            position: absolute;
            display: block;
            width: 24px;
            height: 24px;
            left: -35px;
            top: -3px;
            fill: #888888;
            transition: 0.3s all ease;
        }

        .currency {
            display: inline-block;
            padding: 20px 65px 8px;
            text-align: center;
            color: #f2f4ff;
            text-transform: uppercase;
            font-weight: 300;
        }

        .currencies_vs {
            will-change: transform;
            border-top: 1px solid rgba(242, 244, 255, 0.14);
            position: relative;
            z-index: 2;
            background: #10122d;
            max-height: 100%;

        }

        .currency__data {
            color: #f2f4ff;
            font-size: 12px;
            font-weight: 400;
            line-height: 30px;
            text-transform: uppercase;
        }

        .currencies_vs_account .currency {
            color: #888888;
        }

        .currencies_vs:before,
        .currencies_vs:after {
            content: ' ';
            width: 200px;
            position: absolute;
            top: 0;
            z-index: 2;
            height: 100%;
            display: none;
        }

        .pa-navigation__link {
            position: relative;
            display: inline-block;
            padding: 25px 10px 20px 81px;
            color: #888888;

            font-size: 14px;
            transition: 0.3s all ease;
            border-radius: 2px;
            width: 100%;
        }



        .menu-title {
            color: #fff;
            font-weight: 400;
        }

        .menu-icon {
            font-size: 22px;
            padding-right: 15px;
        }

        .right-nav--small {
            width: 65px;
            overflow: hidden;
        }

        .right-nav--small .pa-navigation__link {
            padding-left: 25px;
            white-space: nowrap;
            color: transparent;
        }

        .right-nav--small .right-nav__title {
            color: transparent;
            white-space: nowrap;
        }

        .social-sidebar-small {
            display: block;
        }

        .pa-right-col {
            margin: 15px 15px 0 348px;
        }

        .pa-breads {
            padding: 17px 10px 12px 46px;
            background: #ffffff url(/logo/logo_icon.svg) no-repeat 16px 16px;
            -webkit-background-size-no: 16px 16px;
            background-size: 16px 16px;
            padding: 16px 16px;
            line-height: 16px;
        }

        .pa-well {
            box-shadow: none;
            border-radius: 2px;
            background-color: #3746ff;
            padding: 32px 0 32px;
            margin-bottom: 15px;
            background-image: url(images/dark/bg_faqeth.svg)
        }

        .bread__item {
            display: block;
            float: left;
            margin-right: 16px;

        }

        .bread__link--current {
            color: #888888;
            text-decoration: none;
        }

        .bread__link {

            font-size: 16px;
            line-height: 22px;
            font-weight: 400;
            color: #e7e9ff;

            position: relative;
        }

        .pa-well--investor {
            background: #3746ff url(images/dark/bg_eth.svg) center center repeat;
            background-size: cover;
        }

        .pa-well--status {
            overflow: hidden;
            position: relative;
        }

        .pa-well--status:before {
            content: ' ';
            position: absolute;
            top: 0;
            transform: translate(0, 85px);
            left: 50%;
            margin-left: -96px;
            width: 192px;
            height: 132px;
            background: url(../img/new/pa_saphire@2x.png);
            background-size: cover;
            z-index: 0;
            opacity: 0;
            will-change: transform;
        }

        .container {
            max-width: none !important;
        }

        .pa-card {
            background: #171a40;
            padding: 32px 20px;
            color: white;
            margin-top: 20px;
            margin-right: 1px;
        }

        .btn-outline.on-bg-theme:before {
            background-color: rgba(32, 148, 250, .17) !important;
        }

        .form-group--copy b {
            background: black url(images/dark/pa_icon_add.svg) no-repeat center;
            -webkit-background-size: 24px 24px;
            background-size: 24px 24px;
            border-radius: 0 24px 24px 0;
            border-radius: 0;
            cursor: pointer;
        }

        .form-group>b {
            width: 64px;
            height: 48px;
            border: none;
            display: block;
            position: absolute;
            right: 0;
            top: 12px;
        }

        .form-group {
            position: relative;
        }

        .card-col {
            padding-left: 0px !important;
        }

        .social-icon-lg {
            width: 64px;
            height: 64px;
            font-size: 2.2em;
            line-height: 64px;
            border-radius: 50%;
            transition: 0.3s;
            color: #28384c;
            background: #fff;
            margin: 25px;
        }

        @media (max-width: 992px) {
            .pa-card {
                margin-left: 1px;
                text-align: center !important;
            }

            .pa-right-col {
                margin: 15px 15px 0px 80px !important;
                z-index: 1;
            }

            .right-nav {
                z-index: 3;
            }

            .form-group {
                margin: 0 20px !important;
            }

            .pa-icon {
                display: none;
            }

            .card-col {
                padding-left: 15px !important;
                margin-top: 15px !important;
            }

            .pa-btn--tab {
                width: auto;
            }

            .tab-list {
                display: inline-block !important;
            }
            .stats-alert{
                margin-left: 5% !important;
                margin-right: 5% !important;
            }

        }

        @media (min-width: 992px) {
            .menu-item {
                padding-right: 30px !important;
            }

            .menu-sub {
                left: -120px !important;
            }
        }

        .tab-list {
            white-space: nowrap;
            font-size: 0;
            margin-bottom: 15px;
            margin-top: 40px;
            justify-content: center;
        }

        .active>.pa-btn,
        .pa-btn--active {
            background-color: #39d884;

            color: white !important;
            text-decoration: none;
            border-color: #555555;
        }

        .pa-btn--tab.pa-btn {
            border-color: #ffffff33;
            color: #2094fa;
            margin-left: -1px;
            margin-right: -1px;
        }

        .pa-btn--tab {
            width: auto;
            padding-left: 22px;
            padding-right: 22px;
            border-radius: 0;
        }

        .pa-btn {
            width: 160px;
            white-space: normal;
            border-radius: 2px;
            border-radius: 24px;
            border-radius: 0;
            border: 1px solid #555555;
            color: #2094fa;
            font-family: "Open Sans";
            font-size: 14px;
            font-weight: 600;
            line-height: 1.2;
            text-decoration: none;
            display: inline-block;
            text-transform: uppercase;
            transition: 0.3s all ease;
            padding: 12px 0 12px;
            background: #0b0c22;
            cursor: pointer;
        }

        /*Transactions*/
        table tr,
        .table thead th {
            border-bottom: none !important;
            border-top: none !important;
